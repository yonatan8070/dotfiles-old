{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "user_info_separator": "",
            "display_host": false,
            "prefix": "<#C41202>┏[</>",
            "postfix": "<#C41202>]</>"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "<#C41202>[</>",
            "postfix": "<#C41202>]</>"
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "<#C41202>[</>",
            "postfix": "<#C41202>]</>",
            "always_enabled": true
          }
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "<#C41202>[</>",
            "postfix": "<#C41202>]</>"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "<#C41202>[x</>",
            "postfix": "<#C41202>]</>"
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "<#C41202>[</>Java ",
            "postfix": "<#C41202>]</>"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#3EA5D9",
          "properties": {
            "prefix": "<#C41202>┖[</>"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "style": "full",
            "prefix": "",
            "postfix": "<#C41202>]></>",
            "enable_hyperlink": true
          }
        }
      ]
    }
  ],
  "final_space": true
}